                                                                <socialmediatitletext
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                    fieldLabel="Social media title text"
                                                                    name="./socialmediatitletext"/>
                                                                <socialmediagenesisurl
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                                    fieldLabel="Social media URL"
                                                                    name="./socialmediagenesisurl"/>
                                                                <socialmediaicon
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                                    fieldLabel="Social media icon"
                                                                    name="./socialmediaicon"
                                                                    rootPath="/content/dam"/>
                                                                <socialmediaalttext
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                    fieldLabel="Social media alt text"
                                                                    name="./socialmediaalttext"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </pages>
                                        </items>
                                    </column>
                                </items>
                            </fieldset>
